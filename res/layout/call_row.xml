<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="1" android:layout_width="-1" android:layout_height="-1">
    <RelativeLayout android:paddingLeft="@dimen/list_row_padding" android:paddingTop="2dp" android:paddingRight="@dimen/list_row_padding" android:paddingBottom="4dp" android:layout_width="-1" android:layout_height="-2">
        <ImageView android:id="@id/call_type_icon" android:paddingTop="6sp" android:layout_width="-2" android:layout_height="-2" android:layout_alignParentLeft="true" />
        <TextView android:textSize="15,5sp" android:textColor="0x106000c" android:id="@id/call_duration" android:layout_width="-2" android:layout_height="-2" android:layout_alignParentRight="true" />
        <LinearLayout android:orientation="1" android:paddingLeft="6dp" android:layout_width="-2" android:layout_height="-2" android:layout_toLeftOf="@id/call_duration" android:layout_toRightOf="@id/call_type_icon">
            <TextView android:textSize="15,5sp" android:textColor="#545454" android:id="@id/call_type" android:layout_width="-1" android:layout_height="-2" />
            <TextView android:textSize="15,5sp" android:textColor="#545454" android:id="@id/call_date" android:layout_width="-1" android:layout_height="-2" />
        </LinearLayout>
    </RelativeLayout>
    <View android:id="@id/divider" android:background="@drawable/divider_gray" android:layout_width="-1" android:layout_height="0,5dp" />
</LinearLayout>
