                    <TextView whatsapp:textSize="@dimen/conversation_participant_name_tv" whatsapp:textColor="#000" whatsapp:id="@id/name_in_group_tv" whatsapp:layout_width="-2" whatsapp:layout_height="-2" whatsapp:lines="1" whatsapp:singleLine="true" />
